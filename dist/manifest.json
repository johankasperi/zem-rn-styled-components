{
  "packageName": "zem-rn-styled-components",
  "name": "React Native Styled Components",
  "description": "Zeplin extension that generates glamorous type styled components for React Native.",
  "version": "0.1.4",
  "author": {
    "name": "Johan Kasperi",
    "email": "johan@kasperi.se",
    "url": "http://kspri.se"
  },
  "options": [
    {
      "name": "Color format",
      "type": "picker",
      "id": "colorFormat",
      "submenu": true,
      "options": [
        {
          "name": "Default",
          "value": "default"
        },
        {
          "name": "HEX",
          "value": "hex"
        },
        {
          "name": "RGB",
          "value": "rgb"
        },
        {
          "name": "HSL",
          "value": "hsl"
        }
      ],
      "default": "default"
    },
    {
      "name": "Add fontscale property to texts",
      "type": "switch",
      "id": "fontScale",
      "default": false
    },
    {
      "name": "Add color property to texts",
      "type": "switch",
      "id": "colorOverride",
      "default": false
    },
    {
      "name": "Text base component",
      "type": "text",
      "id": "textBaseComponent",
      "default": ""
    },
    {
      "name": "Theme namespace for colors in styled components",
      "type": "text",
      "id": "colorThemePrefix",
      "default": ""
    },
    {
      "name": "Theme namespace for fonts in styled components",
      "type": "text",
      "id": "fontThemePrefix",
      "default": ""
    },
    {
      "name": "Path to typography components",
      "type": "text",
      "id": "typographyComponentsPath",
      "default": "StyledComponents/Typography"
    },
    {
      "name": "Path to blur component",
      "type": "text",
      "id": "blurComponentPath",
      "default": ""
    }
  ],
  "projectTypes": [
    "ios",
    "android"
  ],
  "moduleURL": "./main.3b3f171a.js",
  "repository": "https://github.com/johankasperi/zem-rn-styled-components.git",
  "readmeURL": "./README.md"
}